/**
 * 
 */
package io.weather.webapp.api.location.owm;

import javax.persistence.Embeddable;

/**
 * @author christian
 *
 */

@Embeddable
public class Coordinate {

	private double lon;
	private double lat;

	public long getLon() {
		return lon;
	}

	public void setLon(long lon) {
		this.lon = lon;
	}

	public long getLat() {
		return lat;
	}

	public void setLat(long lat) {
		this.lat = lat;
	}

}
