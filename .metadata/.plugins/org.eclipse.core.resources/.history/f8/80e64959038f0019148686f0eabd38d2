/**
 * 
 */
package io.weather.webapp.dto;

import io.weather.webapp.entity.Coordinate;

/**
 * @author christian
 *
 */
public class LocationDTO {

	private long id;
	private String name;
	private String country;
	private Coordinate coord;

	/**
	 * @param id
	 * @param name
	 * @param country
	 * @param coord
	 */
	public LocationDTO(long id, String name, String country, Coordinate coord) {
		super();
		this.id = id;
		this.name = name;
		this.country = country;
		this.coord = coord;
	}

	public long getId() {
		return id;
	}

	public void setId(long id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getCountry() {
		return country;
	}

	public void setCountry(String country) {
		this.country = country;
	}

	public Coordinate getCoord() {
		return coord;
	}

	public void setCoord(Coordinate coord) {
		this.coord = coord;
	}

}
